{
  "parser": "babel-eslint",
  "extends": ["standard"],
  "ecmaFeatures": {
    "modules": true,
    "experimentalObjectRestSpread": true
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "space-before-function-paren": [2, "never"],
    "object-curly-spacing": [2, "always"],
    "array-bracket-spacing": [2, "never"],
    "no-console": 2,
    "valid-jsdoc": 2
  }
}
